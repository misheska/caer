native_include "cvector.hpp";
native_include "cstring.hpp";

file_identifier "TEST";

struct TestStruct {
   num: int32;
   valid: bool;
}

table TestTable {
    length: float;
    node: string;
}

table Test {
	timestamp: int64;
	addressX: int16;
	addressY: int16;
	polarity: bool;
	astr: string;
	aboolvec: [bool];
	aintvec: [int32];
	astrvec: [string];
    ttab : TestTable (native_inline);
    tstru : TestStruct (native_inline);
    ttabvec : [TestTable] (native_inline);
    tstruvec : [TestStruct] (native_inline);
}

table TestPacket {
	events: [Test] (native_inline);
}

root_type TestPacket;
